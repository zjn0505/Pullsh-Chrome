{
  "manifest_version": 2,

  "name": "Pullsh",
  "description": "The extension help you quickly use Pullsh service",
  "version": "1.2",
  
  "icons" : {
		"48" : "icon/ic_pullsh.png",
		"128" : "icon/ic_pullsh_128.png"
	},
  "author": "Jienan Zhang",
  "homepage_url": "http://pullsh.me",
  "short_name": "Pullsh",
  "browser_action": {
    "default_icon": "icon/ic_pullsh.png",
    "default_popup": "popup.html",
    "default_title": "Create a pull or push"
  },
  "permissions": [
    "https://api.jienan.xyz/"
  ]
}